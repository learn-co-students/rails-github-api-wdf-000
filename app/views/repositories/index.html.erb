<div class = "container">
  <h1><%= session[:username] %> - Repos</h1>
  <ul>
  	<% @repos_array.each do |repo| %>
  	<li>
  		<%= repo["name"] %><br>
  	</li>
  	<% end %>
  </ul>

  <%= form_tag("/repositories/create", method: "post") do %>
    <%= label_tag(:name, "Create a New Repo:") %>
    <%= text_field_tag(:name, nil, id: "new-repo") %>
    <%= submit_tag("Create") %>
  <% end %>
</div>